{
  "name": "jupyterlab-quilt",
  "version": "0.2.4",
  "description": "Quilt Data plugin for Jupyter Labs",
  "author": "Calvin Cochran",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/quiltdata/jupyterlab.git"
  },
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.0",
    "@jupyterlab/notebook": "^0.16.0",
    "@types/react": "~16.0.19",
    "@types/react-dom": "~16.0.5",
    "rimraf": "^2.6.1",
    "typescript": "~2.6.0"
  }
}
